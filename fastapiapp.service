[Unit]
Description=React FastAPI Dashboard Web App
After=network.target

[Service]
ExecStart=/srv/React-FastAPI-Dashboard/.venv/bin/uvicorn app.app:app --host 0.0.0.0 --port 8000 --no-access-log
WorkingDirectory=/srv/React-FastAPI-Dashboard
Restart=on-failure
RestartSec=3
User=root
Group=root

#StandardOutput=null
#StandardError=journal
Environment=PYTHONUNBUFFERED=1

[Install]
WantedBy=multi-user.target
